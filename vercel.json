{
  "rewrites": [
    {
      "source": "/(.*).gz",
      "destination": "/"
      "headers": {
        "Content-Type": "application/gzip"
      }
    },
    {
      "source": "/(.*).js",
      "destination": "/"
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
